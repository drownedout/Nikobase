<div class="col-md-8">
	<div class="row">
		<div class="col-md-12">
		  <h1 class="only-bottom-margin bar_border"><%= @company.name %></h1>
		</div>
		</div>
		<div class="row">
		<div class="col-md-10">
			<p class="text-muted"><strong>ID Number:</strong></p>
		  	<p class="text-muted"><strong>Email:</strong></p>
		  	<p class="text-muted"><strong>Employees:</strong></p>
		  	<ul>
		  	<% @users.each do |user| %>
		  		<% if user.company.name == @company.name %>
		  			<li><%=link_to user.firstname, user %> <%=link_to user.lastname, user %></li>
		  		<% end %>
		  	<% end %>
		  	</ul>
		  	<div class="row">
		  		<div class="col-md-6">
		  		<p class="text-muted"><strong>CCS Status:</strong></p> 
		  		</div>
		  		<div class="col-md-6">
		  			<p class="text-muted"><strong>Date:</strong></p> 
		  		</div>
		  	</div>
		  	<div class="row">
		  		<div class="col-md-6">
		  		<p class="text-muted"><strong>CES Status:</strong></p> 
		  		</div>
		  		<div class="col-md-6">
		  			<p class="text-muted"><strong>Date:</strong></p> 
		  		</div>
		  	</div>
		  	<p><strong>Notes:</strong></p>
		  	<div class="box">
		  	<p class="notes_show"></p>
		  	</div>
		</div>
	</div>
	<%= link_to "Edit", edit_company_path, class: "btn btn-primary" %>
	<%= link_to "Back", :back, class: "btn btn-primary" %>
</div>